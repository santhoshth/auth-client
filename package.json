{
  "name": "auth-service-interview",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon --exec npx tsx server/src/app.ts",
    "client:dev": "vite",
    "build": "vite build && npm run server:build",
    "server:build": "tsc -p server/tsconfig.json",
    "test": "npx tsx --test server/src/**/*.test.ts",
    "lint": "eslint .",
    "preview": "vite preview",
    "setup:db": "npx tsx server/src/database/setup.ts",
    "test:jwks": "npx tsx server/src/scripts/test-jwks.ts",
    "test:authorize": "npx tsx server/src/scripts/test-authorize.ts",
    "debug:flow": "npx tsx server/src/scripts/debug-token-flow.ts",
    "debug:token": "npx tsx server/src/scripts/debug-token-flow.ts"
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.2.4",
    "@supabase/supabase-js": "^2.57.4",
    "@types/jwk-to-pem": "^2.0.3",
    "axios": "^1.6.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-jwt": "^8.5.1",
    "express-oauth2-jwt-bearer": "^1.7.1",
    "express-openid-connect": "^2.19.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "jwk-to-pem": "^2.0.7",
    "jwks-client": "^2.0.5",
    "jwks-rsa": "^3.2.0",
    "lucide-react": "^0.344.0",
    "morgan": "^1.10.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sqlite3": "^5.1.6",
    "ts-node": "^10.9.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.4",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/sqlite3": "^3.1.11",
    "@types/uuid": "^9.0.7",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "nodemon": "^3.0.2",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.20.5",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}
